@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Kullanıcı Listesi";
}

<div class="container">
    <h2>Kullanıcı Listesi</h2>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Ad</th>
                <th>Soyad</th>
                <th>E-posta</th>
                <th>Kullanıcı Adı</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var kullanici in Model)
            {
                <tr>
                    <td>@kullanici.Ad</td>
                    <td>@kullanici.Soyad</td>
                    <td>@kullanici.Email</td>
                    <td>@kullanici.UserName</td>
                </tr>
            }
        </tbody>
    </table>

    <div class="mt-4">
        <a asp-action="AdminDashboard" class="btn btn-primary">Geri Dön</a>
    </div>
</div>
